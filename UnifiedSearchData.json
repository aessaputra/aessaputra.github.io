---
layout: null
permalink: /UnifiedSearchData.json
---
[
  {% assign notes = site.notes | where_exp: "item", "item.path contains '_notes/Public'" %}
  {% for note in notes %}
    {
      "id": "note_{{ forloop.index0 }}",
      "title": {{ note.title | jsonify }},
      "content": {{ note.content | strip_html | normalize_whitespace | jsonify }},
      "url": {{ note.url | relative_url | jsonify }},
      "doc": {{ note.title | jsonify }},
      "type": "note",
      "typeLabel": "Note"
    }{% unless forloop.last or site.posts.size == 0 %},{% endunless %}
  {% endfor %}{% if notes.size > 0 and site.posts.size > 0 %},{% endif %}
  {% for post in site.posts %}
    {
      "id": "post_{{ forloop.index0 }}",
      "title": {{ post.title | jsonify }},
      "content": {{ post.content | strip_html | normalize_whitespace | jsonify }},
      "url": {{ post.url | relative_url | jsonify }},
      "doc": {{ post.title | jsonify }},
      "type": "post",
      "typeLabel": "Writing"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
